<?php

class Person {

    // <inject>
    var $first_name;
    var $last_name;
    var $age;
    var $birthday;


    public function getFirstName()
    {
        return $this->first_name;
    }
    // <inject>

    public function getFulName()
    {
        return $this->first_name . ' '  . $this->last_name;
    }
}
